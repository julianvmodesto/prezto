# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

# Customize to your needs...

source /opt/nyt/sbx/.nyt

export HOMEBREW_GITHUB_API_TOKEN=c3a33aae81cc53746a0a89437bbf83975feb7174

# added by Anaconda2 2.5.0 installer
export PATH="/Users/207302/anaconda/bin:$PATH"

# include locally installed Cassandra in PATH
PATH="$PATH:$HOME/Applications/cassandra/bin"

# include Datastax Opscenter for Apache Cassandra and Datastax Enterprise in PATH
PATH="$PATH:$HOME/opscenter/bin"

export GOPATH=$HOME/Code/golang

# enable iTerm shell integraiton
source ~/.iterm2_shell_integration.`basename $SHELL`


# The next line updates PATH for the Google Cloud SDK.
source '/Users/207302/Applications/google-cloud-sdk/path.zsh.inc'

# The next line enables shell command completion for gcloud.
source '/Users/207302/Applications/google-cloud-sdk/completion.zsh.inc'
